{
  "name": "@js-heap-inspector/causal-link-cli",
  "version": "1.0.0",
  "description": "CLI for causal link analysis of V8 heap snapshots",
  "scripts": {
    "build": "tsc",
    "playwright-performance-reporter-converter": "ts-node ./src/playwright-performance-reporter-converter.ts ./data/${TARGET_APP}/base.json --output ./data/${TARGET_APP}/base.heapsnapshot && ts-node ./src/playwright-performance-reporter-converter.ts ./data/${TARGET_APP}/modified.json --output ./data/${TARGET_APP}/modified.heapsnapshot",
    "v8:runtime-converter": "ts-node ./src/runtime-converter.ts",
    "v8:causal-link": "PYTHONPATH=\"$(pwd)/../../packages/@js-heap-inspector-data-science/src\" && python src/causal_link.py --baseline ./data/${TARGET_APP}/base.runtime.json --modified ./data/${TARGET_APP}/modified.runtime.json --codeEvolution ./data/${TARGET_APP}/codeEvolution.json --output ./data/${TARGET_APP}/result.json --outputReporter ./data/${TARGET_APP}/result-reporter",
    "v8:causal-link:community-detection": "yarn run v8:causal-link --settings ./modes/${TARGET_APP}.json",
    "v8:causal-link:primitive": "yarn run v8:causal-link --settings ./modes/primitive.json",
    "v8:causal-link:heuristic-greedy": "yarn run v8:causal-link --settings ./modes/heuristic-greedy.json",
    "v8:full-runtime-converter": "yarn run v8:runtime-converter ./data/${TARGET_APP}/base.heapsnapshot --output ./data/${TARGET_APP}/base.runtime.json && yarn run v8:runtime-converter ./data/${TARGET_APP}/modified.heapsnapshot --output ./data/${TARGET_APP}/modified.runtime.json",
    "v8:full-causal-link:primitive": "yarn run v8:full-runtime-converter && yarn run v8:causal-link:primitive",
    "v8:full-causal-link:heuristic-greedy": "yarn run playwright-performance-reporter-converter && yarn run v8:full-runtime-converter && yarn run v8:causal-link:heuristic-greedy",
    "v8:full-causal-link:community-detection": "yarn run playwright-performance-reporter-converter && yarn run v8:full-runtime-converter && yarn run v8:causal-link:community-detection",
    "v8:full-causal-link:otter-simple-showcase:heuristic-greedy-1": "TARGET_APP=\"otter-simple-showcase-heuristic-greedy-1\" && yarn run v8:full-causal-link:heuristic-greedy",
    "v8:full-causal-link:otter-simple-showcase:community-detection-1": "TARGET_APP=\"otter-simple-showcase-community-detection-1\" && yarn run v8:full-causal-link:community-detection",
    "v8:full-causal-link:otter-simple-showcase:heuristic-greedy-2": "TARGET_APP=\"otter-simple-showcase-heuristic-greedy-2\" && yarn run v8:full-causal-link:heuristic-greedy",
    "v8:full-causal-link:otter-simple-showcase:community-detection-2": "TARGET_APP=\"otter-simple-showcase-community-detection-2\" && yarn run v8:full-causal-link:community-detection",
    "v8:full-causal-link:otter-simple-showcase:heuristic-greedy-3": "TARGET_APP=\"otter-simple-showcase-heuristic-greedy-3\" && yarn run v8:full-causal-link:heuristic-greedy",
    "v8:full-causal-link:otter-simple-showcase:community-detection-3": "TARGET_APP=\"otter-simple-showcase-community-detection-3\" && yarn run v8:full-causal-link:community-detection",
    "v8:full-causal-link:otter-simple-showcase:heuristic-greedy-4": "TARGET_APP=\"otter-simple-showcase-heuristic-greedy-4\" && yarn run v8:full-causal-link:heuristic-greedy",
    "v8:full-causal-link:otter-simple-showcase:community-detection-4": "TARGET_APP=\"otter-simple-showcase-community-detection-4\" && yarn run v8:full-causal-link:community-detection",
    "v8:full-causal-link:otter-simple-showcase:heuristic-greedy-5": "TARGET_APP=\"otter-simple-showcase-heuristic-greedy-5\" && yarn run v8:full-causal-link:heuristic-greedy",
    "v8:full-causal-link:otter-simple-showcase:community-detection-5": "TARGET_APP=\"otter-simple-showcase-community-detection-5\" && yarn run v8:full-causal-link:community-detection",
    "v8:full-causal-link:otter-extensive-showcase:heuristic-greedy-1": "TARGET_APP=\"otter-extensive-showcase-heuristic-greedy-1\" && yarn run v8:full-causal-link:heuristic-greedy",
    "v8:full-causal-link:otter-extensive-showcase:community-detection-1": "TARGET_APP=\"otter-extensive-showcase-community-detection-1\" && yarn run v8:full-causal-link:community-detection",
    "v8:full-causal-link:otter-extensive-showcase:heuristic-greedy-2": "TARGET_APP=\"otter-extensive-showcase-heuristic-greedy-2\" && yarn run v8:full-causal-link:heuristic-greedy",
    "v8:full-causal-link:otter-extensive-showcase:community-detection-2": "TARGET_APP=\"otter-extensive-showcase-community-detection-2\" && yarn run v8:full-causal-link:community-detection",
    "v8:full-causal-link:otter-extensive-showcase:heuristic-greedy-3": "TARGET_APP=\"otter-extensive-showcase-heuristic-greedy-3\" && yarn run v8:full-causal-link:heuristic-greedy",
    "v8:full-causal-link:otter-extensive-showcase:community-detection-3": "TARGET_APP=\"otter-extensive-showcase-community-detection-3\" && yarn run v8:full-causal-link:community-detection",
    "v8:full-causal-link:otter-extensive-showcase:heuristic-greedy-4": "TARGET_APP=\"otter-extensive-showcase-heuristic-greedy-4\" && yarn run v8:full-causal-link:heuristic-greedy",
    "v8:full-causal-link:otter-extensive-showcase:community-detection-4": "TARGET_APP=\"otter-extensive-showcase-community-detection-4\" && yarn run v8:full-causal-link:community-detection",
    "v8:full-causal-link:otter-extensive-showcase:heuristic-greedy-5": "TARGET_APP=\"otter-extensive-showcase-heuristic-greedy-5\" && yarn run v8:full-causal-link:heuristic-greedy",
    "v8:full-causal-link:otter-extensive-showcase:community-detection-5": "TARGET_APP=\"otter-extensive-showcase-community-detection-5\" && yarn run v8:full-causal-link:community-detection"
  },
  "dependencies": {
    "@js-heap-inspector/core": "workspace:*",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.7.3"
  }
}
