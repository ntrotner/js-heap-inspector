+import type { Translation } from '@o3r/core';
+
+export interface LocalizationTranslation extends Translation {
+  /**
+   * Key for the message in the speech bubble until a destination is selected
+   */
+  welcome: string;
+
+  /** Title of the localization page */
+  title: string;
+
+  /** Description title */
+  descriptionTitle: string;
+
+  /** Module description */
+  description: string;
+
+  /** Example title */
+  exampleTitle: string;
+
+  /** Example introduction */
+  exampleIntro: string;
+
+  /** Example destination selection description */
+  exampleDestinationDescription: string;
+
+  /** Locally run description */
+  locallyRunDescription: string;
+
+  /** Locally run instructions link label */
+  locallyRunInstructions: string;
+
+  /** Source code link label */
+  sourceCode: string;
+
+  /** Installation title */
+  installTitle: string;
+
+  /** Component generation title */
+  componentGenerationTitle: string;
+
+  /** Add localization to component title */
+  addLocalizationTitle: string;
+
+  /** Add localization key to component title */
+  addLocalizationKeyTitle: string;
+
+  /** Localization bundle title */
+  bundleTitle: string;
+
+  /** Component bundle generation title */
+  bundleComponentTitle: string;
+
+  /** Component bundle description part 1 */
+  bundleComponentDescription1: string;
+
+  /** Component bundle description part 2 */
+  bundleComponentDescription2: string;
+
+  /** Component bundle description part 3 */
+  bundleComponentDescription3: string;
+
+  /** Check options description */
+  checkOptionsDescription: string;
+
+  /** Application bundle generation title */
+  bundleAppTitle: string;
+
+  /** Application bundle description */
+  bundleAppDescription: string;
+
+  /** Metadata title */
+  metadataTitle: string;
+
+  /** Metadata extraction title */
+  metadataExtractTitle: string;
+
+  /** Metadata extraction description */
+  metadataExtractDescription: string;
+
+  /** References title */
+  referencesTitle: string;
+
+  /** Documentation link label */
+  documentation: string;
+}
+export const translations: LocalizationTranslation = {
+  welcome: 'o3r-localization.welcome',
+  title: 'o3r-localization.title',
+  descriptionTitle: 'o3r-localization.descriptionTitle',
+  description: 'o3r-localization.description',
+  exampleTitle: 'o3r-localization.exampleTitle',
+  exampleIntro: 'o3r-localization.exampleIntro',
